<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SimpleRegistration</servlet-name>
        <servlet-class>Registration.SimpleRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SimpleRegistrationServlet</servlet-name>
        <servlet-class>servlets.SimpleRegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>First</servlet-name>
        <servlet-class>First</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CoachRegistration</servlet-name>
        <servlet-class>Registration.CoachRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SimpleRegServlet</servlet-name>
        <servlet-class>Registration.SimpleRegServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Registration.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Registration.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SimpleRegistration</servlet-name>
        <url-pattern>/SimpleRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SimpleRegistrationServlet</servlet-name>
        <url-pattern>/SimpleRegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>First</servlet-name>
        <url-pattern>/First</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CoachRegistration</servlet-name>
        <url-pattern>/CoachRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SimpleRegServlet</servlet-name>
        <url-pattern>/SimpleRegServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
